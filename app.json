{
  "expo": {
    "name": "eaarth-studios",
    "slug": "eaarth-studios",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/eaarth.png",
    "scheme": "projects",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/images/eaarth.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "bundleIdentifier": "com.anonymous.eaarthstudios"
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/eaarth.png"
      },
      "splash": {
        "image": "./assets/images/eaarth.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.anonymous.eaarthstudios"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/eaarth.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-splash-screen",
        {
          "imageWidth": 160,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#0f172a"
          },
          "image": "./assets/images/eaarth.png"
        }
      ],
      "expo-secure-store",
      "expo-barcode-scanner"
    ]
  }
}
